<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>PodAnalyzer</id>
    <version>0.0.1.0</version>
    <title>PodAnalyzer</title>
    <authors>Rikki Gibson</authors>
    <owners>Rikki Gibson</owners>
    <licenseUrl>https://github.com/RikkiGibson/PodAnalyzer/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/RikkiGibson/PodAnalyzer</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>PodAnalyzer</description>
    <releaseNotes>First version, released as a test.</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>PodAnalyzer, analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>